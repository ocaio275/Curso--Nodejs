<h1>Produtos</h1>
<div class="products">
    {{#each products}}
    <div class="product">
        <img src="{{this.image}}" alt="{{this.name}}"
            onerror="this.onerror=null;this.src='data:image/jpeg;base64,'">
        <h3>{{this.name}}</h3>
        <p>R$: {{this.price}}</p>
        <div class="actions">
            <a href="products/{{this._id}}">Detalhes</a>
            {{!-- <a href="products/delete/{{this._id}}">Remover Produto</a> --}}
        </div>
    </div>
    {{/each}}
</div>