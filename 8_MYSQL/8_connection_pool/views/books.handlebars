<h1>Livros disponiveis: </h1>
<div class="books-container">
    <a href="/">Adicionar livro</a>
    {{#each books}}
        <div>
            #{{this.id}} - 
            <span class="bold">
                <a href="/books/{{this.id}}">
                    {{this.title}}
                </a> - {{this.pageqty}}
            </span>
        </div>
    {{/each}}
</div>